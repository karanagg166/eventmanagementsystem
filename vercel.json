{
    "version": 2,
    "builds": [
      {
        "src": "Client/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "./Server/index.js",
        "use": "@vercel/node"
      }
      
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Server/index.js"
      }
    ]
  }
  